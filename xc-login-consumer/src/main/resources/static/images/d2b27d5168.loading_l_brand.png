<script src="//ms2-m.vipstatic.com/js/e41a4b7a8b.all.min.js"></script>

    <!-- 分仓：LBS弹窗 -->
    <script>
	(function() {
		var curWarehouse = decodeURI(Cookie.Get('warehouse')),
			lbsData;

	    function init() {
	        var opts = {
	        	timeout : 3000,
	        	enableHighAccuracy : true
	        }

	        if (!navigator.geolocation) {
	        	errHandler();
	            return;
	        }

	        // 请求地理位置成功
	        function success(position) {
	            var latitude = position.coords.latitude;
	            var longitude = position.coords.longitude;

	            successHandler('agree-warehouse_btn');
	            successHandler('latlng-req-success_btn');

	        	// 请求百度，经纬度换省份
	        	$.ajax({
	        		url : 'http://api.map.baidu.com/geocoder/v2/',

	        		data : {
	        			ak : 'GsWdp2jUz1tvC6bV0sClDfDX',
	        			output : 'json',
	        			pois : '1',
	        			location : latitude + ',' + longitude
	        		},

	        		dataType : 'jsonp',

	        		success : function(data){
	        			successHandler('baidu-req-success_btn');

	        			$(function(){
	        				var result, province, provinceList, warehouseElem;

			            	if(data.status == 0){
			            		// 百度返回的省份，返回的"广东省"，本地是"广东"，需要把最后的“省”及“市”去掉，如此类推
			            		province = data.result.addressComponent.province.slice(0, -1);

			            		warehouseElem = getWarehouseElem(province);

			            		// 省份匹配成功
			            		if(warehouseElem){
			            			changeWarehouse(warehouseElem);
			    					VIP.localStorage.setItem('changing_warehouse_type', 1)
			    				// 省份匹配失败
			            		} else {
			            			ipLocation();
			            			Mar.Seed.request("button", "click", 'baidu-req-faile_btn');
			            		}
			            		
			            		
			            	}else{
			            		ipLocation();
			            		Mar.Seed.request("button", "click", 'baidu-req-faile_btn');
			            	}
		            	})
	        		}, 

	        		error : function(err){
	        			ipLocation();
			            Mar.Seed.request("button", "click", 'baidu-req-faile_btn');
	        		}
	        	})
	        };

	        // 请求地理位置失败
	        function error (err){
	        	var code = err.code, mar, data;
	        	// 1：用户拒绝，2：无法获取，3：超时
	        	switch(code){
	        		case 1 :
	        			Mar.Seed.request("button", "click", 'refuse-warehouse_btn');
	        			Mar.Seed.request("button", "click", 'latlng-req-faile_btn', 'user');
	        			ipLocation();
	        			break;
	        		case 2 :
	        			Mar.Seed.request("button", "click", 'agree-warehouse_btn');
	        			Mar.Seed.request("button", "click", 'latlng-req-faile_btn', 'unknow');
	        			ipLocation();
	        			break;
	        		case 3 :
	        			Mar.Seed.request("button", "click", 'agree-warehouse_btn');
	        			Mar.Seed.request("button", "click", 'latlng-req-faile_btn', 'timeout');
	        			ipLocation();
	        			break;
	        	} 
	        }

	       	// 通过省份名，获取分仓弹窗对应元素
	       	function getWarehouseElem(province){
	       		var list, matchElem;

        		// 获取分仓弹窗里的省份列表
        		list = $('.ui-list-li li');
        		list.each(function(k, v){
        			var link = $('a', v);
        			if(province == $.trim(link.html())){
        				matchElem = link;
        				return false;
        			}
        		})

        		return matchElem;
	       	}


	        function ipLocation(){
	        	var ipLocation = vipConfig.ipLocation, warehouseElem;

	        	warehouseElem = getWarehouseElem(ipLocation.province);

	        	if(ipLocation.result == 1 && warehouseElem){
        			changeWarehouse(warehouseElem);
	        	} else {
	        		manualWarehouse();
	        	}
	        }

	        // 通过分仓元素，切换分仓
	        function changeWarehouse(warehouseElem){
	        	hawk.depot.set({
					dom: {
						elem : warehouseElem,
    					posi : warehouseElem.data('positions'),
    					area : encodeURI(warehouseElem.text())
					}
				});
	        }

	        function successHandler(mar){
	            Mar.Seed.request("button", "click", mar);
	        }

	        function errHandler(mar){
	        	Mar.Seed.request("button", "click", mar);
	        	manualWarehouse();
	        }

	        // 手动分仓弹窗
	        function manualWarehouse(){
		        hawk.depot.show('#w-modal', {modalHide : true});
	        }

	        Mar.Seed.request("button", "click", 'lbs-location_btn');
	        navigator.geolocation.getCurrentPosition(success, error, opts);
	        
	    }

	    $(function(){
	    	if(Cookie.Get('WAP[p_wh]')){
		    	$('.J_nav_warehouse').show().find('.warehouse_txt').html(decodeURI(Cookie.Get('WAP[p_area]')));
		    }else{
		    	init();
		    }
	    })
	    

	})()
</script>

